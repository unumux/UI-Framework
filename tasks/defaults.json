{
  "setup": {
    "sourceSansSrc": "UI-Framework/fonts/sourceSansPro/*",
    "sourceSansDest": "fonts/sourceSansPro/",
    "oswaldSrc": "UI-Framework/fonts/oswald/*",
    "oswaldDest": "fonts/oswald/"
  },
  "watch": {
    "scss": [
      "scss/*.scss"
    ],
    "js": [
      "js/**/*.js",
      "!js/modernizr/*.js"
    ],
    "html": [
      "*.html"
    ]
  },
  "debug": {
    "js": [
      "js/script-agile.js"
    ],
    "jsPath": [
      "//localhost:35729/livereload.js",
      "bower_components/jquery/dist/jquery.min.js",
      "js/**/*.js",
      "!js/site.js",
      "!js/site.min.js"
    ],
    "cssPath": "css/site.css",
    "masterLayout": "index.html"
  },
  "dev": {
    "scss": "scss/site.scss",
    "css": "css/site.css",
    "jsSource": [
      "bower_components/jquery/dist/jquery.min.js",
      "js/**/*.js",
      "!js/site.js",
      "!js/site.min.js"
    ]
  },
  "release": {
    "js": "js/site.min.js",
    "jsPath": "js/site.min.js",
    "css": "css/site.min.css",
    "cssPath": "css/site.min.css",
    "modernizr": "js/modernizr/modernizr-custom.js"
  }
}